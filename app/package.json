{
  "name": "nw-screensaver",
  "description": "A web-based screensaver with transparency and multi-monitor support",
  "version": "1.5.4",
  "repository": {
    "type": "git",
    "url": "http://github.com/1j01/nw-screensaver.git"
  },
  "bugs": {
    "url": "http://github.com/1j01/nw-screensaver/issues"
  },
  "license": "MIT",
  "keywords": [
    "screensaver",
    "eyesaver",
    "eye-saver",
    "eyecandy",
    "eye-candy",
    "multi-monitor",
    "dual-screen",
    "tri-screen",
    "quad-screen",
    "wow-screen",
    "wow",
    "screen",
    "monitor",
    "display",
    "epic",
    "fullscreen",
    "transparency",
    "node-webkit",
    "nw.js",
    "nwjs",
    "nw",
    "app",
    "desktop"
  ],
  "dependencies": {
    "react": "^0.13.2",
    "react-script": "^1.2.1"
  },
  "devDependencies": {
    "nw-dev": "^1.3.1"
  },
  "main": "app.html",
  "window": {
    "title": "Screensaver",
    "icon": "img/icon.png",
    "transparent": true,
    "frame": false,
    "resizable": false,
    "visible_on_all_workspaces": true,
    "show": false
  },
  "webkit": {
    "plugin": true
  }
}
